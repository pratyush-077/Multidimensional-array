#include <iostream>
using namespace std;

int main() {
     int rows1,rows2 , rows3 ;
     int cols1 ,cols2 , cols3 ;
     
    cin>>rows1;
    cin>>cols1;
    cin>>rows2;
    cin>>cols2;
    cin>>rows3;
    cin>>cols3;
    
    int M1[rows1][cols1];
    int M2[rows2][cols2];
    int M3[rows3][cols3];
    
   
    cout << "Enter the elements of the 3x3 matrix M1:" << endl;

    for (int i = 0; i < rows1; i++) {
        for (int j = 0; j < cols1; j++) {
            cin >> M1[i][j];
        }
    }

    cout << "The first matrix is:" << endl;

    for (int i = 0; i < rows1; i++) {
        for (int j = 0; j < cols1; j++) {
            cout << M1[i][j] << " ";
        }
        cout << endl;
    }
    
    cout << "Enter the elements of the 3x3 matrix M2:" << endl;

    for (int k = 0; k < rows2; k++) {
        for (int l = 0; l < cols2; l++) {
            cin >> M2[k][l];
        }
    }

    cout << "The second matrix is:" << endl;

    for (int k= 0; k < rows2; k++) {
        for (int l = 0; l < cols2; l++) {
            cout << M2[k][l] << " ";
        }
        cout << endl;
    }
    
    if(rows1==rows2 && cols1==cols2){
       cout<<M3[rows3][cols3];
        for (int k = 0; k < rows2; k++) {
          for (int l = 0; l < cols2; l++) {
             M3[rows3][cols3]= M1[rows1][cols1] + M2[rows2][cols2];
             cout<<M3[rows3][cols3]<< " ";
        }
        cout << endl;
      }
    }

    return 0;
}



